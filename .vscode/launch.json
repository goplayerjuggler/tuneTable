  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch custom script",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\tmp\\s.mjs"
      // "${workspaceFolder}\\test\\manipulation.js"
    },
